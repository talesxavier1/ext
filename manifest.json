{
  "name": "script injection",
  "version": "0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://jsonformatter.org/"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self' 'wasm-unsafe-eval';"
  }
}